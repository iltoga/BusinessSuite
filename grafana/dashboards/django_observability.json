                    "datasource": {
                        "type": "loki",
                        "uid": "${DS_LOKI}"
                    },
                    "editorMode": "code",
                    "expr": "sum by (level) (count_over_time({service=~\"$service\", service=~\".+\"} |~ \"(?i)$search\" [$__interval]))",
