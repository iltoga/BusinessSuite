                        },
                        "thresholdsStyle": {
                            "mode": "off"
                        }
                    },
                    "mappings": []
                },
                "overrides": [
                    {
                        "matcher": {
                            "id": "byRegexp",
                            "options": "/.*ERROR.*/i"
                        },
                        "properties": [
                            {
                                "id": "color",
                                "value": {
                                    "fixedColor": "red",
                                    "mode": "fixed"
                                }
                            }
                        ]
                    },
                    {
                        "matcher": {
                            "id": "byRegexp",
                            "options": "/.*INFO.*/i"
                        },
                        "properties": [
                            {
                                "id": "color",
                                "value": {
